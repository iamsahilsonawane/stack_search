{
  "name": "personal-links-manager",
  "displayName": "Personal links manager",
  "description": "Personal Links Manager!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:personal-links-manager.getQuestionsFromSO",
    "onCommand:personal-links-manager.getFavQuestionsFromSO"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Personal Links Manager",
      "properties": {
        "linksManager.config.getFavoriteTags": {
          "type": "string",
          "default": "StackOverflow",
          "description": "Add comma-separated favorite tags",
          "examples": [
            "flutter,dart",
            "python,pycharm"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "personal-links-manager.getQuestionsFromSO",
        "title": "Search Stack Overflow"
      },
      {
        "command": "personal-links-manager.getFavQuestionsFromSO",
        "title": "Favorite Search Stack Overflow"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  },
  "dependencies": {
    "axios": "^0.23.0"
  }
}
